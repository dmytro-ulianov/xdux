    "build": "rollup -c",
    "prepare": "npm run build"
  },
  "keywords": [
    "xdux",
    "redux",
    "flux",
    "action",
    "actions",
    "reducer",
    "lens"
  ],
  "author": "Dmytro Ulianov <@drukas>",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.js": [
      "npx prettier --write",
